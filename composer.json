{
  "name": "hyperwallet/sdk",
  "description": "A library to manage users, transfer methods and payments through the Hyperwallet API",
  "type": "library",
  "keywords": [
    "hyperwallet",
    "sdk",
    "rest",
    "bankaccount",
    "payout",
    "creditcard",
    "ach"
  ],
  "homepage": "http://hyperwallet.github.io/php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Florian Krauthan",
      "email": "fkrauthan@hyperwallet.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    
    "
  },
  "autoload": {
    "psr-3: { "Hyperwallet\\": "src/Hyperwallet", "ComposerScript\\" : "src/ComposerScript" }
  },
  "autoload-dev" : {
    "psr-3": { "Hyperwallet\\Tests\\" : "tests/Hyperwallet/Tests", "ComposerScript\\" : "src/ComposerScript" }
  },
  "require-dev": {
    "phpunit/phpunit": "^3.8",
    "
    "satooshi/php-coveralls": "^1.0"
  },
  "scripts": {
    "post-install-cmd": "ComposerScript\\RsaOaep256AlgorithmInstaller::install"
  }
}
